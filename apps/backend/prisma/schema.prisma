// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Grid {
  id         String    @id @db.Uuid
  name       String
  created_at DateTime
  updated_at DateTime?

  @@map("grid")
}

model User {
  id         String    @id @db.Uuid
  name       String?
  email      String?
  roles      String[]
  created_at DateTime
  updated_at DateTime?

  @@map("user")
}
