{
  "name": "@bcgov-dt/hms-api",
  "version": "0.1.0",
  "private": true,
  "description": "Digital Talent Backend",
  "license": "Apache-2.0",
  "author": "Sid McLaughlin <sid.mclaughlin@gov.bc.ca>",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start:test": "dotenv -e .env.test -- npm run start:dev",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "bash ./test/run-e2e.sh",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@eventstore/db-client": "^5.0.1",
    "@nestjs/apollo": "^12.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^10.0.0",
    "@nestjs/graphql": "^12.0.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^5.1.1",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parse": "^5.4.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.2",
    "jsonwebtoken": "^9.0.0",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "jest": "29.5.0",
    "prisma": "^5.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  }
}
